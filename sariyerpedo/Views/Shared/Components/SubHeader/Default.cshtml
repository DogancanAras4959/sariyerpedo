@using sariyerpedo.Resource
@model sariyerpedo.BUSSINES.DTOS.SiteSettingData.SiteSettingDto

@inject LocalizationService localizationService

<style>
    #mainnav .menu {
        padding-left: 30px;
    }
    .header2 #mainnav .menu > li {
        margin-right: 30px;
    }
</style>

<div class="top-bar">
    <div class="inner jus-ct">
        <p class="clr-pri-1">@localizationService.GetLocalizedHtmlString("Working Hours") 08:00 am - 05:00 pm</p>
    </div>
</div>
<header id="site-header" class="header_main header2">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="site-header-inner fx">
                    <div id="site-logo" class="clearfix">
                        <a href="@Url.Action("sayfa","anasayfa")" class="logo">
                            <img src="~/assets/images/logo/logodark.png" alt="Kinco">
                        </a>
                    </div>
                    <div class="btn-menu"><span></span></div>
                    <div class="nav-wrap">
                        <nav id="mainnav" class="mainnav">
                            <ul class="menu">
                                <li class="menu-item">
                                    <a href="@Url.Action("sayfa","anasayfa")">@localizationService.GetLocalizedHtmlString("anasayfa")</a>
                                </li>
                                <li class="menu-item">
                                    <a href="/tedaviler">@localizationService.GetLocalizedHtmlString("tedaviler")</a>
                                </li>
                                <li class="menu-item">
                                    <a href="/doktorumuz">@localizationService.GetLocalizedHtmlString("doktorumuz")</a>
                                </li>
                                <li class="menu-item">
                                    <a href="/klinigimiz">@localizationService.GetLocalizedHtmlString("kliniğimiz")</a>
                                </li>
                                <li class="menu-item">
                                    <a href="/iletisim">@localizationService.GetLocalizedHtmlString("iletişim")</a>
                                </li>
                                @*<li class="menu-item">
                                    <a href="/randevual">@localizationService.GetLocalizedHtmlString("randevual")</a>
                                </li>*@
                                <li class="menu-item">
                                    @await Html.PartialAsync("_setLanguagePartial")
                                </li>
                            </ul>
                            <!-- /.menu -->
                        </nav>
                    </div>
                    <div class="header-right fx">
                        <div class="header-contact fx">
                            <div class="inner-contact fx">
                                <svg data-name="Hero Area" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="49" height="47" viewBox="0 0 49 47">
                                    <g data-name="Menu Area">
                                        <g>
                                            <image width="49" height="47" xlink:href="data:img/png;base64," />
                                        </g>
                                    </g>
                                </svg>
                                <ul>
                                    <li class="clr-pri-2">@localizationService.GetLocalizedHtmlString("ContactUs")</li>
                                    <li class="clr-pri-2">+012 (345) 678</li>
                                </ul>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</header>